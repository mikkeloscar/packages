pkgname=libgcc-dev-armhf-cross-bin
_basever=4.8
pkgver=${_basever}.2
_ubuntuver=${pkgver}-16ubuntu4cross0.11
pkgrel=1
pkgdesc="Development GCC support library for armhf architecture"
arch=('any')
url="http://packages.ubuntu.com/libgcc-${_basever}-dev-armhf-cross"
license=('GPL2')
options=('!strip' 'staticlibs')
source=("http://archive.ubuntu.com/ubuntu/pool/main/g/gcc-${_basever}-armhf-cross/libgcc-${_basever}-dev-armhf-cross_${_ubuntuver}_all.deb")
md5sums=('09d74e8aa5a50405093bb638a14834ad')

package() {
    cd $pkgdir
    tar xf $srcdir/data.tar.xz
}
