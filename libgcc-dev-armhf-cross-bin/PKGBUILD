pkgname=libgcc-dev-armhf-cross-bin
_basever=4.8
pkgver=${_basever}.1
_ubuntuver=${pkgver}-10ubuntu7cross0.10
pkgrel=1
pkgdesc="Development GCC support library for armhf architecture"
arch=('any')
url="http://packages.ubuntu.com/libgcc-${_basever}-dev-armhf-cross"
license=('GPL2')
options=('!strip')
source=("http://archive.ubuntu.com/ubuntu/pool/universe/g/gcc-${_basever}-armhf-cross/libgcc-${_basever}-dev-armhf-cross_${_ubuntuver}_all.deb")
md5sums=('f2fc825f7c8bd890d6e2771e8b72b318')

package() {
    cd $pkgdir
    tar xf $srcdir/data.tar.gz
}
